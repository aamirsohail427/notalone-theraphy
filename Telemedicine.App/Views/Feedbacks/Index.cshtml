
@{
    ViewData["Title"] = "Feedbacks";
}
@section styles{
    <link rel="stylesheet" type="text/css" href="~/app-assets/css/pages/page-blog.css">
    <style type="text/css">
        div.display-flex img {
            max-width: 50px !important;
            min-width: 50px !important;
            width: 100%;
            max-height: 50px !important;
            min-height: 50px !important;
            height: 100% !important;
            -moz-border-radius: 50%;
            -webkit-border-radius: 50%;
            background-position: 50% 50%;
            background-repeat: no-repeat;
            object-fit: cover;
        }

        .feedback-description {
            min-height: 80px;
            max-height: 80px;
            height: 100%;
            overflow: auto;
            display: block;
        }

            .feedback-description::-webkit-scrollbar-track {
                -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
                /*border-radius: 10px;*/
                background-color: #F5F5F5;
            }

            .feedback-description::-webkit-scrollbar {
                width: 3px;
                /*background-color: #F5F5F5;*/
            }

            .feedback-description::-webkit-scrollbar-thumb {
                /*border-radius: 10px;*/
                -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);
                background-color: #f2f4f8;
            }
    </style>
}
@section scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            getFeedbacks();
        });
        function getFeedbacks() {
            $.get("../Feedbacks/Get").done(function (data) {
                $("#feedbacks_detail").html(data);
            });
        }
    </script>
}
